<%= form_for @user, url:'signup' do |f| %>
    <%= f.label :name, "Username" %>
    <%= f.text_field :name %>
<br>
    <%= f.label :password %>
    <%= f.text_field :password %>
<br>
    <%= f.label :password_confirmation %>
    <%= f.text_field :password_confirmation %>
<br>
    <%= f.submit 'Signup' %>
<% end %> 